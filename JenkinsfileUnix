pipeline { 
    agent any 
    stages 
    {
        stage('Checkout From SCM') 
        { 
            steps 
                {
                        sh "git fetch https://github.com/Michael-Masas/Bakery.git"
                }
        }
        stage('Build')
        {
            steps
                {
                sh "cd /var/jenkins_home/workspace/Maven-Build/Bakery"
                }
                {
                sh "mvn install"
                }
                
        }
    }
}
